{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@formkit/auto-animate": "^1.0.0-beta.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-regular-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "@headlessui/react": "^1.5.0",
    "@heroicons/react": "^1.0.5",
    "@jridgewell/trace-mapping": "^0.2.3",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.1",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^0.24.0",
    "classnames": "^2.3.1",
    "dayjs": "^1.11.2",
    "eslint": "^8.19.0",
    "joi": "^17.6.0",
    "joi-phone-number": "^5.1.1",
    "jsonpath": "^1.1.1",
    "rc-slider": "^10.0.0",
    "react": "^18.2.0",
    "react-canvas-confetti": "^1.3.0",
    "react-confetti": "^6.0.1",
    "react-countdown-circle-timer": "^3.0.9",
    "react-debounce-input": "^3.2.5",
    "react-dom": "^18.2.0",
    "react-dom-confetti": "^0.2.0",
    "react-markdown": "^8.0.1",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.0",
    "react-spring": "^9.4.4",
    "react-toastify": "^9.0.3",
    "react-usa-map": "^1.5.0",
    "react-use": "^17.3.2",
    "react-use-window-size": "^1.0.1",
    "remark-gfm": "^3.0.1",
    "tailwind-merge": "^1.2.1",
    "tailwind-react-datepicker": "^1.1.4",
    "use-debounce": "^8.0.1",
    "web-vitals": "^2.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "rules": {
      "react/prop-types": "off",
      "indent": [
        "error",
        4,
        {
          "SwitchCase": 1
        }
      ],
      "linebreak-style": [
        "error",
        "windows"
      ],
      "quotes": [
        "error",
        "double"
      ],
      "semi": [
        "error",
        "always"
      ],
      "semi-style": [
        "error",
        "last"
      ],
      "curly": [
        "error",
        "multi-line"
      ],
      "max-depth": [
        "error",
        4
      ],
      "multiline-comment-style": [
        "error",
        "separate-lines"
      ],
      "no-eval": "error",
      "no-lone-blocks": "error",
      "no-var": "error",
      "arrow-spacing": "error",
      "block-spacing": "error",
      "func-call-spacing": [
        "error",
        "never"
      ],
      "key-spacing": [
        "error",
        {
          "beforeColon": false,
          "afterColon": true
        }
      ],
      "brace-style": [
        "error",
        "1tbs",
        {
          "allowSingleLine": true
        }
      ],
      "space-before-blocks": "error",
      "no-irregular-whitespace": "error",
      "func-style": [
        "error",
        "expression"
      ],
      "max-statements": [
        "error",
        22
      ],
      "no-confusing-arrow": "error",
      "no-empty-function": "error",
      "comma-spacing": [
        "error",
        {
          "before": false,
          "after": true
        }
      ],
      "comma-style": [
        "error",
        "last"
      ],
      "dot-location": [
        "error",
        "property"
      ],
      "implicit-arrow-linebreak": [
        "error",
        "beside"
      ],
      "jsx-quotes": [
        "error",
        "prefer-double"
      ],
      "keyword-spacing": [
        "error"
      ],
      "no-trailing-spaces": "error",
      "no-whitespace-before-property": "error",
      "object-curly-newline": [
        "error",
        {
          "multiline": true
        }
      ],
      "rest-spread-spacing": [
        "error",
        "never"
      ],
      "space-infix-ops": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.2",
    "postcss": "^8.4.5",
    "tailwindcss": "^3.0.13"
  },
  "description": "\"# Alumni_Web_React_Frontend\"",
  "main": "postcss.config.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/baylorbridges/baylor-bridges-frontend.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/baylorbridges/baylor-bridges-frontend/issues"
  }
}
